  "dependencies": {
    "fomantic-ui-css": "2.7.2",
    "postinstall": "0.7.5"
  },
  "devDependencies": {
    "@pageboard/pagecut": "0.10.1",
    "cropperjs": "1.5.12",
    "duration-relativetimeformat": "2.0.2",
    "fast-safe-stringify": "2.1.1",
    "postinstall-bundle": "0.7.5",
    "pretty-bytes": "5.6.0",
    "prosemirror-dev-tools": "3.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "speakingurl": "14.0.1"
  },
  "postinstall": {
    "fomantic-ui-css/components": "link lib/components",
    "fomantic-ui-css/themes": "link lib/themes"
  },
  "prepare": {
    "cropperjs/dist/cropper.css": "copy lib/",
    "cropperjs/dist/cropper.js": "copy lib/",
    "speakingurl/lib/speakingurl.js": "copy lib/",
    "@pageboard/pagecut/src/editor.js": "bundle --name=window.Pagecut lib/pagecut/",
    "@pageboard/pagecut/src/menubar.js": "bundle --name=window.Pagecut.Menubar lib/pagecut/menu.js",
    "@pageboard/pagecut/src/*.css": "copy lib/pagecut/",
    "./src/pageboard.js": "bundle --name=window.Pageboard.utils lib/"
  },
  "pageboard": {
    "priority": -1,
    "directories": [
      "elements",
      "lib",
      "ui"
    ],
    "elements": [
      "elements/"
    ]
  }
}
